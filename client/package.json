{
  "name": "nasa-system6-client",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "axios": "^1.6.0",
    "d3": "^7.8.5",
    "framer-motion": "^10.16.16",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:unit": "react-scripts test --testPathPattern=__tests__ --testPathIgnorePatterns=integration",
    "test:services": "npx jest --config jest.config.services.js",
    "test:integration": "react-scripts test --testPathPattern=integration --testTimeout=30000",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:watch": "react-scripts test --watchAll",
    "test:user-resources": "node src/__tests__/testRunner.js all",
    "test:user-resources:unit": "node src/__tests__/testRunner.js unit",
    "test:user-resources:integration": "node src/__tests__/testRunner.js integration",
    "test:user-resources:coverage": "node src/__tests__/testRunner.js coverage",
    "eject": "react-scripts eject",
    "lint": "eslint src --config eslint.config.js",
    "lint:fix": "eslint src --config eslint.config.js --fix",
    "format": "prettier --write src",
    "format:check": "prettier --check src",
    "validate": "npm run lint:all && npm run format:all && npm run test",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "homepage": "https://thunderbird-esq.github.io/stylesnprofiles",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@eslint/js": "^9.39.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^30.2.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "gh-pages": "^6.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "prettier": "^3.6.2"
  },
  "overrides": {
    "nth-check": "^2.1.1",
    "postcss": "^8.4.31"
  }
}